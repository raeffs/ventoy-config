[
  {
    "pattern": "^fedora-server-dvd-(?<architecture>\\S{6})-(?<version>\\w{2})-\\S*\\.iso$",
    "alias": "Fedora Server ${version}"
  },
  {
    "pattern": "^fedora-workstation-live-(?<architecture>\\S{6})-(?<version>\\w{2})-\\S*\\.iso$",
    "alias": "Fedora Workstation ${version}"
  },
  {
    "pattern": "^truenas-(?<version>\\S*)\\.iso$",
    "alias": "True NAS ${version}"
  },
  {
    "pattern": "^ubuntu-(?<version>\\S*)-desktop-(?<architecture>\\w{5})\\.iso$",
    "alias": "Ubuntu Desktop ${version}"
  },
  {
    "pattern": "^ubuntu-(?<version>\\S*)-live-server-(?<architecture>\\w{5})\\.iso$",
    "alias": "Ubuntu Server ${version}"
  },
  {
    "pattern": "^(?<language>\\S{2,5})_windows_10_business_editions_version_(?<version>\\w{4})(_updated_\\S*)?_(?<architecture>\\w{3})_dvd_\\S*\\.iso$",
    "alias": "Windows 10 Build ${version}"
  },
  {
    "pattern": "^(?<language>\\S{2,5})_windows_server_version_(?<version>\\w{4})(_updated_\\S*)?_(?<architecture>\\w{3})_dvd_\\S*\\.iso$",
    "alias": "Windows Server ${version}"
  }
]
